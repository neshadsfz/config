#take arg
echo "loading config ... "

###########################
###########################
####### LIB SHELL #########
###########################
###########################

tmuxConf(){
	if [[ -z "$TMUX" ]]; then
    if tmux has-session 2>/dev/null; then
        exec tmux attach
    else
        exec tmux
    fi
fi
}
########end of conf########
echo -e "done configuring\n"
